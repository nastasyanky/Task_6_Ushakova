<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <AnchorPane prefHeight="224.0" prefWidth="277.0">
         <children>
            <TextField fx:id="tx_bubble" layoutX="16.0" layoutY="119.0" />
            <TextField fx:id="tx_selection" layoutX="16.0" layoutY="154.0" />
            <TextField fx:id="tx_insertion" layoutX="16.0" layoutY="187.0" prefHeight="17.0" prefWidth="149.0" />
            <Button fx:id="b_bubble" layoutX="180.0" layoutY="119.0" mnemonicParsing="false" onAction="#bubbleSort" prefHeight="25.0" prefWidth="90.0" text="Пузырь" />
            <Button fx:id="b_selection" layoutX="180.0" layoutY="154.0" mnemonicParsing="false" onAction="#selectionSort" prefHeight="25.0" prefWidth="90.0" text="Выбор" />
            <Button fx:id="b_insertion" layoutX="180.0" layoutY="187.0" mnemonicParsing="false" onAction="#insertionSort" prefHeight="25.0" prefWidth="90.0" text="Вставка" />
            <TextField fx:id="tx_array" layoutX="14.0" layoutY="34.0" prefHeight="25.0" prefWidth="253.0" />
            <Label layoutX="85.0" layoutY="12.0" text="Исходный Массив" />
            <Label layoutX="57.0" layoutY="97.0" text="Отсортированные масссивы" />
            <Button fx:id="b_array" layoutX="99.0" layoutY="65.0" mnemonicParsing="false" onAction="#on_array" text="Заполнить" />
         </children>
      </AnchorPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
